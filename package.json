{
  "name": "pnpm-a12n-server-next",
  "version": "1.0.0",
  "description": "A sample pnpm workspace with a12n-server and Next.js app",
  "main": "index.js",
  "scripts": {
    "start:app": "pnpm --filter \"app\" dev",
    "start:server": "pnpm --filter \"a12n-server\" start",
    "build:server": "pnpm --filter \"a12n-server\" build",
    "build": "pnpm --filter \"app\" build && pnpm build:server ",
    "lint": "pnpm --filter \"a12n-server\" fix && pnpm --filter \"app\" lint"
  },
  "keywords": [],
  "author": "usrrname",
  "license": "ISC",
  "engines": {
    "node": ">=20.10.0"
  },
  "packageManager": "pnpm@9.15.4"
}
